{
  "name": "toolhive-studio",
  "productName": "ToolHive",
  "version": "0.0.0-dev",
  "description": "ToolHive is an application that allows you to install, manage and run MCP servers and connect them to AI agents and clients",
  "main": ".vite/build/main.js",
  "repository": "https://github.com/stacklok/toolhive-studio",
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "scripts": {
    "start": "electron-forge start",
    "e2e": "tsc -b --clean && tsc -b && electron-forge package && playwright test",
    "start:mockUpdate": "MOCK_UPDATE_SERVER=true ELECTRON_IS_DEV=0 electron-forge start",
    "package": "tsc -b --clean && tsc -b && electron-forge package",
    "make": "tsc -b --clean && tsc -b && electron-forge make",
    "prettier": "prettier . --check",
    "format": "prettier . --write",
    "lint": "eslint .",
    "publish": "electron-forge publish",
    "type-check": "tsc --noEmit -p ./tsconfig.app.json && tsc --noEmit -p ./tsconfig.node.json",
    "prepare": "husky install",
    "thv": "ts-node ./utils/fetch-thv.ts --run",
    "test": "vitest",
    "test:nonInteractive": "vitest run",
    "test:coverage": "vitest run --coverage",
    "update-api": "ts-node scripts/update-api.ts",
    "generate-client": "pnpm run update-api && cd renderer && openapi-ts",
    "generate-client:nofetch": "cd renderer && openapi-ts",
    "generate-icons": "tsx scripts/generate-icons.ts",
    "knip": "knip",
    "playwright": "playwright"
  },
  "keywords": [],
  "license": "Apache-2.0",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-base": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-flatpak": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron-forge/publisher-github": "^7.8.1",
    "@electron-forge/shared-types": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.25.0",
    "@hey-api/openapi-ts": "0.78.3",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4.1.10",
    "@tanstack/router-plugin": "^1.120.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/regexp.escape": "^2.0.0",
    "@types/unzipper": "^0.10.11",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/coverage-istanbul": "^3.1.4",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.5.0",
    "electron": "37.2.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "icon-gen": "^5.0.0",
    "jsdom": "^26.1.0",
    "knip": "^5.58.1",
    "lint-staged": "^16.0.0",
    "msw": "^2.8.4",
    "prettier": "3.6.2",
    "prettier-plugin-classnames": "^0.8.0",
    "prettier-plugin-merge": "^0.8.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tar": "^7.4.3",
    "testcontainers": "^11.2.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.30.1",
    "unzipper": "^0.12.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4",
    "vitest-fail-on-console": "^0.7.1"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.6",
